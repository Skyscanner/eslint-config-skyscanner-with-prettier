{
  "name": "eslint-config-skyscanner-with-prettier",
  "version": "0.7.0",
  "description": "Skyscanner's eslint config with prettier support ",
  "repository": {
    "type": "git",
    "url": "https://github.com/Skyscanner/eslint-config-skyscanner-with-prettier"
  },
  "main": "index.js",
